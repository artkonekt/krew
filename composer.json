{
    "name": "konekt/krew",
    "description": "Krew is a Concord box that handles HR data",
    "type": "library",
    "keywords": ["laravel", "concord", "concord-box"],
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/krew"
    },
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.5",
        "konekt/appshell": "~1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4.2",
        "phpunit/phpunit" : "6.2 - 7.4",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": { "Konekt\\Krew\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Krew\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
